        <div class="card preview @((OldTheme == "zephyr") ? "text-white bg-dark" : "bg-light")">
            <div class="image">
                <a href="@(BootswatchUrl)zephyr/" target="_blank">
                    <img src="img/theme-preview/zephyr.png" alt="Zephyr">
                </a>
            </div>
            <div class="options">
                <h3>Zephyr</h3>
                <p>Breezy and beautiful</p>
                <div class="btn-group">
                    @{
                        destinationUrl = $"{BootswatchUrl}/zephyr/";
                    }
                    <Button Color="Color.Primary" Type="ButtonType.Link" To="@destinationUrl" Target="Target.Blank">@Localizer["BtnPreview"]</Button>
                </div>
                <div class="btn-group">
                    <Button Color="Color.Success" Clicked="@(() => SelectTheme("zephyr"))" Disabled="@((OldTheme == "zephyr") ? true : false)">@Localizer["BtnSelect"]</Button>
                </div>
            </div>
        </div>
    </div>

</div>