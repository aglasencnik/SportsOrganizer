                <a href="@(BootswatchUrl)default/" target="_blank">
                    <img src="img/theme-preview/default.png" alt="Default">
                </a>
            </div>
            <div class="options">
                <h3>Default</h3>
                <p>Basic Bootstrap</p>
                <div class="btn-group">
                    @{
                        string destinationUrl = $"{BootswatchUrl}/default/";
                    }
                    <Button Color="Color.Primary" Type="ButtonType.Link" To="@destinationUrl" Target="Target.Blank">@Localizer["BtnPreview"]</Button>
                </div>
                <div class="btn-group">
                    <Button Color="Color.Success" Clicked="@(() => SelectTheme("default"))" Disabled="@((OldTheme == "default") ? true : false)">@Localizer["BtnSelect"]</Button>
